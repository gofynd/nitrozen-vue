{
  "name": "@gofynd/nitrozen-vue",
  "version": "0.0.52",
  "description": "Fynd Design Library for Vue",
  "homepage": "https://github.com/gofynd/nitrozen-vue#readme",
  "license": "MIT",
  "private": false,
  "main": "dist/nitrozen.umd.js",
  "module": "dist/nitrozen.umd.js",
  "unpkg": "dist/nitrozen.umd.min.js",
  "types": "./types/index.d.ts",
  "keywords": [
    "nitrozen",
    "vue",
    "fynd",
    "gofynd",
    "nitrozen-vue"
  ],
  "scripts": {
    "serve": "npm run tasks && vue-cli-service serve --port 4000",
    "build": "npm run tasks && vue-cli-service build",
    "build-website": "rm -rf public && vue-cli-service build --dest public src/main.js",
    "build-bundle": " npm run tasks && vue-cli-service build --target lib --name nitrozen src/entry-lib.js",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "tasks": "node tasks"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "dependencies": {
    "animejs": "^3.2.1",
    "core-js": "^3.26.0",
    "hammerjs": "^2.0.8",
    "json-logic-js": "^2.0.2",
    "shortid": "^2.2.16",
    "vue": "^2.6.14",
    "vue-tel-input": "^2.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^9.2.0",
    "css-loader": "^7.1.2",
    "express": "^4.18.2",
    "less": "^4.2.0",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.9.1",
    "vue-template-compiler": "^2.6.14",
    "webpack-merge": "^5.8.0"
  }
}
